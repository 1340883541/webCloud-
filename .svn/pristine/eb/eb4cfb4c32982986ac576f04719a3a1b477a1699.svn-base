/* 滑动验证码 */
.vue-auth-box {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 999;
  opacity: 1;
  pointer-events: auto;
  -webkit-transition: opacity 200ms;
  transition: opacity 200ms;
}
.vue-auth-box .vcode-box {
  padding: 10px;
  position: absolute;
  background: #fff;
  top: -moz-calc((100vh - 162px) / 2) !important;
  top: -webkit-calc((100vh - 162px) / 2) !important;
  top: calc((100vh - 162px) / 2) !important;
  left: -moz-calc(63%) !important;
  left: -webkit-calc(63%) !important;
  left: calc(63%) !important;
  background: #fff;
  border-radius: 4px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
}

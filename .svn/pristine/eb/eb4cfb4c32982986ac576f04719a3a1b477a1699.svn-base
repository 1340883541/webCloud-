    arial,
    \5b8b\4f53,
    "Helvetica Neue",
    Helvetica,
    STHeiTi,
    sans-serif;
  background: #f7f7f7;
  font-size: 14px;
  color: #333;
  padding: 0;
  margin: 0;
  /* font-weight:400; */
}

body {
  -webkit-text-size-adjust: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.padding {
  padding: 30px 20px 30px 20px;
}

.color-chengse {
  color: #f3a135;
}

.content-container::-webkit-scrollbar-track-piece {
  /* background: transparent; */
}

/* 侧栏滚动条 */
.el-scrollbar__wrap {}

/*清除浮动*/
.clearfix:before,
.clearfix:after {
  display: table;
  content: " ";
}

.clearfix:after {
  clear: both;
}

.clearfix {
  *zoom: 1;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

/*弹性盒子布局*/
.flex-wrap {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
}

.flex-wrap1 {
  display: -webkit-box;
}

.flex-con {
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
}

.flex-hh {
  -webkit-flex-wrap: wrap;
  -moz-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  -o-flex-wrap: wrap;
  flex-wrap: wrap;
}

.flex-hh-not {
  -webkit-flex-wrap: nowrap;
  -moz-flex-wrap: nowrap;
  -ms-flex-wrap: nowrap;
  -o-flex-wrap: nowrap;
  flex-wrap: nowrap;
}

.flex-vertical {
  -webkit-box-orient: vertical;
  -webkit-flex-flow: column;
  flex-flow: column;
}

.flex-center {
  justify-content: center;
}

.flex-end {
  justify-content: flex-end;
}

.flex-between {
  justify-content: space-between;
}

.flex-align-center {
  align-content: center;
}

.flex-align-between {
  align-content: space-between;
}

.flex-align-item {
  align-items: center;
}

.flex-align-item-end {
  align-items: end;
}

.flex-align-item-flex-end {
  align-items: flex-end;
}

/*强制换行*/
.break-word {
  word-wrap: break-word;
}

/* 多行省略 */
.elipseone {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.w-elli {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.w-elli-2 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}

.w-elli-3 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  text-overflow: ellipsis;
}

.w-elli-4 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 4;
  overflow: hidden;
  text-overflow: ellipsis;
}

.w-elli-5 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 5;
  overflow: hidden;
  text-overflow: ellipsis;
}


